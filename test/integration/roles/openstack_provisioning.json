{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]",
    "recipe[openstack_test::hosts_provisioning]",
    "recipe[openstack_test::cacert]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "ceph": {
      "network": {
        "public": {
          "cidr": [
            "10.1.100.0/23"
          ]
        },
        "cluster": {
          "cidr": [
            "10.1.100.0/23"
          ]
        }
      }
    },
    "openstack": {
      "mq": {
        "user": "admin"
      },
      "compute": {
        "metering": true,
        "libvirt": {
          "virt_type": "qemu"
        }
      },
      "image": {
        "image_upload": true,
        "upload_image": {
          "cirros": "http://download.cirros-cloud.net/daily/20150923/cirros-d150923-x86_64-disk.img"
        },
        "upload_images": [
          "cirros"
        ]
      }
    }
  },
  "default_attributes": {
    "mysql": {
      "server_root_password": "rootpass",
      "server_debian_password": "debpass",
      "server_repl_password": "replpass",
      "allow_remote_root": true,
      "root_network_acl": [
        "%"
      ]
    },
    "osl-openstack": {
      "database_suffix": "x86",
      "databag_prefix": "x86",
      "ceph": true,
      "endpoint_hostname": "controller.example.com",
      "db_hostname": "controller.example.com",
      "nova_public_key": "ssh-rsa  nova-migration",
      "physical_interface_mappings": [
        {
          "name": "public",
          "controller": {
            "default": "eth0"
          },
          "compute": {
            "default": "eth0"
          }
        }
      ]
    }
   },
  "json_class": "Chef::Role",
  "description": "Openstack x86 Provisioning Role",
  "name": "openstack_provisioning"
}
