{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "openstack": {
      "db": {
        "service_type": "mariadb"
      },
      "image": {
        "image_upload": true,
        "upload_image": {
          "cirros": "http://download.cirros-cloud.net/daily/20150923/cirros-d150923-x86_64-disk.img"
        },
        "upload_images": [
          "cirros"
        ]
      },
      "mq": {
        "user": "admin"
      },
      "compute": {
        "metering": true,
        "libvirt": {
          "virt_type": "qemu"
        }
      }
    }
  },
  "default_attributes": {},
  "json_class": "Chef::Role",
  "description": "Openstack x86 Test Kitchen Role",
  "name": "openstack_tk"
}
