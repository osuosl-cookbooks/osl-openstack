# This file is automatically generated by Chef
# Any changes will be overwritten

[vlans]
tenant_network_type = gre,vxlan
network_vlan_ranges =

[vxlan]
enable_vxlan = true
l2_population = true
local_ip = <%= @local_ip %>

[agent]
polling_interval = 2

[securitygroup]
enable_security_group = True
firewall_driver = neutron.agent.linux.iptables_firewall.IptablesFirewallDriver

[linux_bridge]
physical_interface_mappings = <%= @physical_interface_mappings.join(',') %>
